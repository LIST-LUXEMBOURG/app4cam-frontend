{
  "name": "app4cam-frontend",
  "version": "5.3.2",
  "productName": "App4Cam",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "quasar build",
    "check": "vue-tsc --noEmit --skipLibCheck",
    "concurrently:dev:vitest": "concurrently \"quasar dev\" \"vitest\"",
    "dev": "quasar dev",
    "format": "prettier --write --ignore-unknown .",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "test:unit:ci": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:watch": "vitest"
  },
  "dependencies": {
    "@quasar/extras": "1.16.14",
    "apexcharts": "4.1.0",
    "axios": "1.7.9",
    "date-fns": "3.6.0",
    "date-fns-tz": "3.2.0",
    "pinia": "2.3.0",
    "quasar": "2.17.4",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue3-apexcharts": "1.8.0"
  },
  "devDependencies": {
    "@pinia/testing": "0.1.7",
    "@quasar/app-vite": "2.0.0-beta.24",
    "@quasar/vite-plugin": "1.8.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/user-event": "14.5.2",
    "@testing-library/vue": "8.1.0",
    "@types/node": "22.10.1",
    "@typescript-eslint/eslint-plugin": "8.17.0",
    "@typescript-eslint/parser": "8.17.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/coverage-v8": "2.1.8",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-testing-library": "7.1.0",
    "eslint-plugin-vitest": "0.4.1",
    "eslint-plugin-vue": "9.32.0",
    "husky": "9.1.7",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.10",
    "prettier": "3.4.2",
    "typescript": "5.4.5",
    "vite-plugin-checker": "0.6.4",
    "vite-tsconfig-paths": "5.1.3",
    "vitest": "2.1.8",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0",
    "npm": ">= 6.13.4"
  },
  "license": "GPL-3.0-or-later",
  "author": {
    "name": "Daniel Waxweiler",
    "email": "daniel.waxweiler@list.lu"
  }
}
